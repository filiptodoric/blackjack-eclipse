<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<script src="../blackjack/deck.js"></script>
<script>

	var index = 0;
	var deck = getDeck();
	function deal() {
		return deck[index];
	}

	function change() {
		var card = deal();
		index++;
		console.dir(card);
		$("#dealer-cards").html(card);
	}
</script>
<body>
<h1>Blackjack</h1>
  <div id="dealer">Dealer
  	<h3>Dealer Cards:</h3>
  	<button onclick="change()">Deal</button>
  	<div id="dealer-cards">
  		
  	</div>
  </div>
